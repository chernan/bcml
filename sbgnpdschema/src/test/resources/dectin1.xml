			</Complex>
			<Association>
				<Connection refNode="13betaglucan">
					<Consumption />
				</Connection>
				<Connection refNode="Dectin-1">
					<Consumption />
				</Connection>
				<Connection refNode="13betaglucandecrin1">
					<Consumption />
				</Connection>
			</Association>
			<Macromolecule ID="Dectin-1_" cloneref="Dectin-1">
